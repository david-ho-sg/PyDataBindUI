precision mediump float;

uniform sampler2D CC_Texture0;

varying vec2 v_texCoord0;
varying vec2 v_texCoord1;
varying vec2 v_texCoord2;
varying vec2 v_texCoord3;
varying vec2 v_texCoord4;

void main(void)
{
	vec4 color = vec4(0.0);
    color += texture2D(CC_Texture0, v_texCoord0) * 0.19947;
    color += texture2D(CC_Texture0, v_texCoord1) * 0.16408;
    color += texture2D(CC_Texture0, v_texCoord2) * 0.16408;
    color += texture2D(CC_Texture0, v_texCoord3) * 0.09132;
    color += texture2D(CC_Texture0, v_texCoord4) * 0.09132;

    gl_FragColor = color;
}
