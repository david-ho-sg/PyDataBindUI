precision mediump float;
#ifndef USE_SEPARATE_ALPHA
 #define USE_SEPARATE_ALPHA 0
#endif

uniform sampler2D CC_Texture0;
#if USE_SEPARATE_ALPHA
uniform sampler2D CC_Texture1;
#endif
varying lowp vec4 use_color;
varying mediump vec2 v_texture0;
void main()
{
float CC_Texture0_bias = 0.0;
float CC_Texture1_bias = 0.0;
lowp vec4 local_0 = texture2D(CC_Texture0, v_texture0, CC_Texture0_bias);
lowp vec4 local_1;
#if USE_SEPARATE_ALPHA
lowp vec4 local_2 = texture2D(CC_Texture1, v_texture0, CC_Texture1_bias);
lowp vec3 local_3 = local_0.xyz;
lowp float local_4 = local_0.w;
lowp float local_5 = local_2.x;
lowp float local_6 = local_2.y;
lowp float local_7 = local_2.z;
lowp float local_8 = local_2.w;
lowp vec4 local_9 = vec4(local_3.x, local_3.y, local_3.z, local_5);
lowp vec4 local_10 = use_color * local_9;
local_1 = local_10;
#else
lowp vec4 local_11 = use_color * local_0;
local_1 = local_11;
#endif
