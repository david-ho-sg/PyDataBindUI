precision mediump float;
uniform float CC_alpha_value;
uniform sampler2D CC_Texture0;
varying lowp vec4 v_diffuse;
varying mediump vec2 v_texture0;
void main()
{
float CC_Texture0_bias = 0.0;
lowp vec4 local_0 = texture2D(CC_Texture0, v_texture0, CC_Texture0_bias);
lowp vec4 local_1 = local_0 * v_diffuse;
gl_FragColor = local_1;
lowp vec3 local_2 = local_0.xyz;
lowp float local_3 = local_0.w;
if (local_3 - CC_alpha_value <= 0.0) 
{ 
discard;
} 
}
